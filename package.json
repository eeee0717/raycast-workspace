{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "workspace",
  "title": "Workspace",
  "description": "Manage your workspace",
  "icon": "command-icon.png",
  "author": "eeee",
  "license": "MIT",
  "commands": [
    {
      "name": "my-workspace",
      "title": "My Workspace",
      "subtitle": "Workspace",
      "description": "View your workspace",
      "mode": "view"
    },
    {
      "name": "create-workspace",
      "title": "Create Workspace",
      "subtitle": "Workspace",
      "description": "Create your workspace",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
